{
  "name": "@umm/recorder",
  "version": "1.1.0",
  "description": "Recorder framework that allows recording anything in unity.",
  "homepage": "https://github.com/umm-projects/recorder",
  "bugs": {
    "url": "https://github.com/umm-projects/recorder/issues"
  },
  "scripts": {
    "umm:init": "./node_modules/.bin/umm-init",
    "umm:install": "./node_modules/.bin/umm-install",
    "umm:uninstall": "./node_modules/.bin/umm-uninstall",
    "postinstall": "npm run --silent umm:install",
    "postuninstall": "npm run --silent umm:uninstall",
    "build:test": "tar -tf $(npm pack -s) && $(npm bin)/rimraf $_"
  },
  "author": "Tetsuya Mori",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:umm-projects/recorder"
  },
  "files": [
    "Assets",
    "scripts"
  ],
  "dependencies": {
    "@umm/scripts": "umm-projects/scripts#^1.0.0",
    "glob": "^7.1.2",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {}
}
